<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/ file:///usr/share/openbox/menu.xsd">
    <menu id="root-menu" label="Openbox 3">
        <separator label=" Main Menu "/>
        <menu id="essentials-menu" label=" Essentials">
            <separator label=" Essentials"/>
            <item label=" Terminal">
                <action name="Execute">
                    <execute>urxvt</execute>
                </action>
            </item>
            <item label=" File Manager">
                <action name="Execute">
                    <execute>thunar</execute>
                </action>
            </item>
        </menu>
        <menu id="internet-menu" label=" Internet">
            <seperator label=" Internet"/>
            <item label=" Firefox">
                <action name="Execute">
                    <execute>firefox</execute>
                </action>
            </item>
            <item label="ﭮ Discord">
                <action name="Execute">
                    <execute>discord</execute>
                </action>
            </item>
        </menu>
        <menu id="multimedia-menu" label=" Multimedia">
            <separator label=" Multimedia"/>
            <item label=" GIMP">
                <action name="Execute">
                    <execute>gimp</execute>
                </action>
            </item>
            <item label="ﱘ NCMPCPP">
                <action name="Execute">
                    <execute>urxvt -g 40x40 -e ncmpcpp</execute>
                </action>
            </item>
        </menu>
        <menu id="utilities-menu" label=" Utilities">
            <separator label=" Utilities"/>
            <item label=" Screenshot">				
                <action name="Execute">
                    <execute>scrot</execute>
		</action>
            </item>
        </menu>
        <separator label=""/>
        <item label=" Merge">
            <action name="Execute">
                <execute>xrdb -merge .Xresources</execute>
            </action>
        </item>
        <item label=" Reconfigure">
            <action name="Reconfigure"/>
        </item>
        <item label=" Exit">
            <action name="Exit">
                <prompt>yes</prompt>
            </action>
        </item>
    </menu>    
</openbox_menu>
